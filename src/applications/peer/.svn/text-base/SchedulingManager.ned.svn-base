package simpleoverlay.applications.peer;

import simpleoverlay.applications.peer.ChunkSchedulerInterface;
import simpleoverlay.applications.peer.PeerSchedulerInterface;

module SchedulingManager
{
    parameters:
        @display("i=block/user");
        bool debugFlag = default(false);

    submodules:
        chunkScheduler: <chunkSchedulerModule> like ChunkSchedulerInterface {
            @display("p=80,100");
        }
        peerScheduler: <peerSchedulerModule> like PeerSchedulerInterface {

            @display("p=80,200");
        }
}


